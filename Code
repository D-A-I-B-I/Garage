int A=0;
int B=1;
int C=2;
int D=3;
int E=4;
int F=5;
int G=6;
int H=7;
int I=8;
int J=9;

int cle[] = {1,3,4,6,8};
int cl[10] = {1,5,3,4,8,9,5,2,4,7};
  


void clo(){
  delay(1000);
  for (int i=0; i <= 10; i++){
    if (i+1 <= sizeof(cle))
    {
        cl[i] = abs(cl[i] - cle[i]);
    }  
   Serial.print(i);
   Serial.print(":");
   Serial.println (cl[i]);
  }

}


void setup ()
{
  Serial.begin (9600) ;
 
}
 
void loop () 
{
    clo();
 
}
 
